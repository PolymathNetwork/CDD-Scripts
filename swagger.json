{
    "schemes": [
      "http"
    ],
    "swagger": "2.0",
    "info": {
      "description": "Provider Identity APIs",
      "title": "Provider Identity",
      "contact": {
        "email": "support@polymath.network"
      },
      "license": {
        "name": "Apache 2.0",
        "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
      },
      "version": "0.1.0"
    },
    "host": "test-uis.polymesh.live",
    "basePath": "/",
    "paths": {
      "/healthz": {
        "get": {
          "tags": [
            "healthz"
          ],
          "operationId": "getHealthz",
          "responses": {
            "200": {
              "description": "success"
            },
            "500": {
              "description": "failure"
            }
          }
        }
      },
      "/identity": {
        "post": {
          "security": [
            {
              "basicAuth": []
            }
          ],
          "description": "Creates a new uID if the PII provided has not been used already.\n String values for 'payload_value' MUST each be hashed with sha512. Strings should be UTF-8.\n For check name 'BaseIndividual', expected 'payload_name' are 'FirstName', 'LastName', 'BirthDate', 'CountryAlpha2'.\n For check name 'BaseBusiness', expected 'payload_name' are 'BusinessNumber', 'CountryAlpha2'\n",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "tags": [
            "identity"
          ],
          "summary": "Create a new uID",
          "operationId": "createuID",
          "parameters": [
            {
              "description": "The PII Payloads required to populate the check(s) specified",
              "name": "IdentityChecks",
              "in": "body",
              "required": true,
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/Check"
                }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "object",
                "properties": {
                  "uID": {
                    "type": "string",
                    "format": "uuid"
                  }
                }
              }
            },
            "401": {
              "description": "unauthorized",
              "schema": {
                "$ref": "#/definitions/Unauthorized"
              }
            },
            "403": {
              "description": "forbidden",
              "schema": {
                "$ref": "#/definitions/Forbidden"
              }
            },
            "422": {
              "description": "invalid request",
              "schema": {
                "$ref": "#/definitions/Invalid"
              }
            },
            "500": {
              "description": "error",
              "schema": {
                "$ref": "#/definitions/Error"
              }
            }
          }
        }
      },
      "/identity/check": {
        "post": {
          "security": [
            {
              "basicAuth": []
            }
          ],
          "description": "Returns match values for each check supplied for the uID supplied",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "tags": [
            "identity"
          ],
          "summary": "Check to see id a set of PII fingerprints match the provided uID",
          "operationId": "checkuID",
          "parameters": [
            {
              "description": "The PII Payloads required to match the identity check(s) specified",
              "name": "checks",
              "in": "body",
              "required": true,
              "schema": {
                "type": "object",
                "properties": {
                  "checks": {
                    "type": "array",
                    "items": {
                      "$ref": "#/definitions/Check"
                    }
                  },
                  "uID": {
                    "type": "string",
                    "format": "uuid"
                  }
                }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/IdentityCheckResponse"
                }
              }
            },
            "401": {
              "description": "unauthorized",
              "schema": {
                "$ref": "#/definitions/Unauthorized"
              }
            },
            "403": {
              "description": "forbidden",
              "schema": {
                "$ref": "#/definitions/Forbidden"
              }
            },
            "422": {
              "description": "invalid request",
              "schema": {
                "$ref": "#/definitions/Invalid"
              }
            },
            "500": {
              "description": "error",
              "schema": {
                "$ref": "#/definitions/Error"
              }
            }
          }
        }
      },
      "/identity/search": {
        "post": {
          "security": [
            {
              "basicAuth": []
            }
          ],
          "description": "Searches for identity matches based on the PII payloads provided",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json"
          ],
          "tags": [
            "identity"
          ],
          "summary": "Create a new uID",
          "operationId": "searchuID",
          "parameters": [
            {
              "description": "The PII Payloads to search for matches",
              "name": "body",
              "in": "body",
              "required": true,
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/Check"
                }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/IdentityCheckResponse"
                }
              }
            },
            "401": {
              "description": "unauthorized",
              "schema": {
                "$ref": "#/definitions/Unauthorized"
              }
            },
            "403": {
              "description": "forbidden",
              "schema": {
                "$ref": "#/definitions/Forbidden"
              }
            },
            "422": {
              "description": "invalid request",
              "schema": {
                "$ref": "#/definitions/Invalid"
              }
            },
            "500": {
              "description": "error",
              "schema": {
                "$ref": "#/definitions/Error"
              }
            }
          }
        }
      },
      "/provider/checkTypes": {
        "get": {
          "security": [
            {
              "basicAuth": []
            }
          ],
          "description": "Returns all uniqueness checks currently available to the provider",
          "produces": [
            "application/json"
          ],
          "tags": [
            "provider"
          ],
          "summary": "Returns the checks available to a provider",
          "operationId": "getCheckTypes",
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/CheckType"
                }
              }
            },
            "401": {
              "description": "unauthorized",
              "schema": {
                "$ref": "#/definitions/Unauthorized"
              }
            },
            "403": {
              "description": "forbidden",
              "schema": {
                "$ref": "#/definitions/Forbidden"
              }
            },
            "422": {
              "description": "invalid request",
              "schema": {
                "$ref": "#/definitions/Invalid"
              }
            },
            "500": {
              "description": "error",
              "schema": {
                "$ref": "#/definitions/Error"
              }
            }
          }
        }
      },
      "/provider/rateLimits": {
        "get": {
          "security": [
            {
              "basicAuth": []
            }
          ],
          "description": "Returns the rate limits for provider, current usage and TTL",
          "produces": [
            "application/json"
          ],
          "tags": [
            "provider"
          ],
          "summary": "Returns the rate limit usage for provider",
          "operationId": "getRateLimits",
          "responses": {
            "200": {
              "description": "successful operation",
              "schema": {
                "type": "object",
                "properties": {
                  "TTL": {
                    "type": "integer"
                  },
                  "rateLimit": {
                    "type": "integer"
                  },
                  "rateUsage": {
                    "type": "integer"
                  }
                }
              }
            },
            "401": {
              "description": "unauthorized",
              "schema": {
                "$ref": "#/definitions/Unauthorized"
              }
            },
            "403": {
              "description": "forbidden",
              "schema": {
                "$ref": "#/definitions/Forbidden"
              }
            },
            "422": {
              "description": "invalid request",
              "schema": {
                "$ref": "#/definitions/Invalid"
              }
            },
            "500": {
              "description": "error",
              "schema": {
                "$ref": "#/definitions/Error"
              }
            }
          }
        }
      }
    },
    "definitions": {
      "Check": {
        "type": "object",
        "properties": {
          "check_name": {
            "type": "string"
          },
          "pii_payloads": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/PiiPayload"
            }
          }
        }
      },
      "CheckProviderDetails": {
        "type": "object",
        "properties": {
          "provider_name": {
            "type": "string"
          },
          "provider_url": {
            "type": "string"
          }
        }
      },
      "CheckType": {
        "type": "object",
        "properties": {
          "check_description": {
            "type": "string"
          },
          "check_name": {
            "type": "string"
          },
          "check_version": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      },
      "Forbidden": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      },
      "IdentityCheckResponse": {
        "type": "object",
        "properties": {
          "CheckProvider": {
            "$ref": "#/definitions/CheckProviderDetails"
          },
          "MatchPercentage": {
            "type": "integer",
            "format": "int64"
          },
          "check_name": {
            "type": "string"
          }
        }
      },
      "Invalid": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      },
      "PiiPayload": {
        "type": "object",
        "properties": {
          "payload_name": {
            "type": "string"
          },
          "payload_value": {
            "type": "string"
          }
        }
      },
      "Unauthorized": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      }
    },
    "securityDefinitions": {
      "basicAuth": {
        "type": "basic"
      }
    },
    "tags": [
      {
        "description": "APIs for core identity services",
        "name": "Identity",
        "externalDocs": {
          "description": "Find out more",
          "url": "https://polymath.network"
        }
      }
    ],
    "externalDocs": {
      "description": "Find out more about Identity Providers",
      "url": "http://polymath.network"
    }
  }